<div class="row">
  <div class="small-12 large-6 columns header-right">
    <h1>Log in</h1>
    <%= form_tag sessions_path do %>
      <p>
        <%= label_tag :email %>
        <%= text_field_tag :email, params[:email] %>
      </p>
      <p>
        <%= label_tag :password %>
        <%= password_field_tag :password %>
      </p>
      <p><%= submit_tag "Log in" %></p>
    <% end %>
  </div>
</div>
